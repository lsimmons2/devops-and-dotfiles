"*":
  "atom-clock":
    dateFormat: "MM/DD h:mm a"
    showClockIcon: true
  core:
    excludeVcsIgnoredPaths: true
    telemetryConsent: "no"
  editor:
    fontSize: 11
    softWrap: true
  "markdown-preview":
    useGitHubStyle: true
  "project-manager":
    prettifyTitle: false
  "tree-view":
    hideVcsIgnoredFiles: false
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
    removeTrailingWhitespace: false
".js.source":
  editor:
    commentEnd: "*/}"
    commentStart: "{/*"
